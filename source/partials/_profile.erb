<header class="profile">
  <img class="avatar" src="/images/avatar.png"/>
  <h1><%= site_title %></h1>
  <h2><%= site_subtitle %></h2>
</header>
<hr>
<p><%= profile_text %></p>

<div class="social">
  <div class="social-left">
    <a class="social-link" href="https://github.com/<%= github_username %>"><span class="icon-github"></span> GitHub</a>
    <a class="social-link" href="https://keybase.io/<%= keybase_username %>"><span class="icon-key"></span> Keybase</a>
  </div>
  <div class="social-center">
    <a class="social-link" href="https://twitter.com/<%= twitter_username %>"><span class="icon-twitter"></span> Twitter</a>
    <a class="social-link" href="https://www.linkedin.com/in/<%= linkedin_username %>"><span class="icon-linkedin"></span> LinkedIn</a>
  </div>
  <div class="social-right">
    <a class="social-link" href="http://www.last.fm/user/<%= lastfm_username %>"><span class="icon-lastfm"></span> Last.fm</a>
    <a class="social-link" href="https://open.spotify.com/user/<%= spotify_username %>"><span class="icon-spotify"></span> Spotify</a>
  </div>
</div>
